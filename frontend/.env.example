# URL of the orchestrator service (API + worker).
# Default when running locally: http://localhost:3000
# For GCP deployment, use the Firebase App Hosting URL
VITE_ORCHESTRATOR_URL="http://localhost:3000"

# Alternative: unified API URL (preferred for GCP deployment)
# VITE_API_URL="https://orchestrator--magic-bracket-simulator.us-central1.hosted.app"

# URL of the Log Analyzer service (for raw/condensed/structured logs).
# Default when running locally: http://localhost:3001
# Note: In GCP deployment, log analyzer routes are served by the orchestrator
VITE_LOG_ANALYZER_URL="http://localhost:3001"

# Firebase Configuration
# Get these values from Firebase Console > Project Settings > Your apps > Web app
VITE_FIREBASE_API_KEY="your-api-key"
VITE_FIREBASE_AUTH_DOMAIN="magic-bracket-simulator.firebaseapp.com"
VITE_FIREBASE_PROJECT_ID="magic-bracket-simulator"
VITE_FIREBASE_STORAGE_BUCKET="magic-bracket-simulator.appspot.com"
VITE_FIREBASE_MESSAGING_SENDER_ID="your-sender-id"
VITE_FIREBASE_APP_ID="your-app-id"
